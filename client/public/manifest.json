{
  "manifest_version": 3,
  "name": "Owl Watch",
  "description": "Manage active tabs, sessions, and save memory.",
  "version": "1.0",
  "permissions": [
    "identity",
    "tabs",
    "storage",
    "alarms",
    "activeTab",
    "scripting",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
 "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/logo16.png",
      "48": "icons/logo48.png",
      "128": "icons/logo128.png"
    }
  },
  "icons": {
     "16": "icons/logo16.png",
      "48": "icons/logo48.png",
      "128": "icons/logo128.png"
  },
  "oauth2": {
    "client_id": "507224401037-mp7gp6kl93bjnoquu6psdfmmibu4v281.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email",
      "openid"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ]
}